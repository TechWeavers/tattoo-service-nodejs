<div class="main-panel">
    <div class="content-wrapper">
        <div class="row">
            <button class="btn btn-primary col-2 m-3"><a href="/novo-colaborador" style="color:inherit">Novo Colaborador</a></button>
        </div>
        <div class="row">
            <div class="col-lg-12 grid-margin stretch-card">
                <div class="card">
                    <div class="card-body">
                        <h4 class="card-title">Lista de Colaboradores</h4>
                        <div class="table-responsive">
                            <table class="table table-striped">
                                <thead>
                                    <tr>
                                        <th>ID</th>
                                        <th>Nome</th>
                                        <th>CPF</th>
                                        <th>Telefone</th>
                                        <th>Email</th>
                                        <th>Rede</th>
                                        <th>Tipo</th>
                                        <th>Editar</th>
                                    </tr>
                                </thead>
                                {{#each colaboradores}} {{#dataValues}} 
                                <tbody>
                                    <tr>
                                        <td class="py-1">{{id_colaborador}}</td>
                                        <td>{{nome}}</td>
                                        <td>{{cpf}}</td>
                                        <td>{{telefone}}</td>
                                        <td>{{email}}</td>
                                        <td>{{redeSocial}}</td>
                                        <td>{{tipo}}</td>
                                        <td>
                                            <button class="btn btn-secondary"><a href="/editar-colaborador/{{id_colaborador}}" style="color:inherit">Edit</a></button>
                                            <button type="button" class="btn btn-primary" id="confirmDelete"><a href="/excluir-colaborador/{{id_colaborador}}" style="color:inherit">Delete</a></button>
                                        </td>
                                    </tr>
                                </tbody>
                                {{/dataValues}} {{/each}}
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
